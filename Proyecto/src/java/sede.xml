<struts>
    <package name="sede" extends="struts-default">   
        <action name="sedeAlta" class="actividades.Actions.sede" >
            <result name="success">/vista/sedeAlta.jsp</result>
        </action>
        
        <action name="altaSede" class="actividades.Actions.sede" method="altaSede" >
            <result name="success">/vista/indexSede.jsp</result>
        </action>
        
        <action name="indexSede" class="actividades.Actions.sede" method="listaSede">
            <result name="success">/vista/indexSede.jsp</result>
        </action>
        
        <action name="eliminarSede" class="actividades.Actions.sede" method="eliminarSede">
            <result name="success">/vista/indexSede.jsp</result>
        </action>
        <action name="editSede" class="actividades.Actions.sede" method="editSede">
            <result name="success">/vista/editSede.jsp</result>
        </action>
        <action name="editSedeF" class="actividades.Actions.sede" method="editSedeF">
            <result name="success">/vista/indexSede.jsp</result>
        </action>
    </package>
</struts>
