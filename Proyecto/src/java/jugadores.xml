<struts>
   <package name="jugador" extends="struts-default">   
        <action name="indexJugador" class="actividades.Actions.gestionJugadores" >
            <result name="success">/vista/jugadores/gestionarJugadores.jsp</result>
        </action>
        
        <action name="eliminarJugador" class="actividades.Actions.gestionJugadores" method="eliminarJugador">
            <result name="success">/vista/jugadores/gestionarJugadores.jsp</result>
        </action>
        <action name="editarJugador" class="actividades.Actions.gestionJugadores" method="editarJugador">
            <result name="success">/vista/jugadores/editarJugador.jsp</result>
        </action>
        <action name="altaJugador" class="actividades.Actions.gestionJugadores" method="altaJugador">
            <result name="success">/vista/jugadores/editarJugador.jsp</result>
        </action>
        
    </package>
</struts>
